<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.gtevents.gtevents" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".eventcreationscreen.EventCreationScreenActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name=".feedscreen.FeedScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".eventdetailscreen.EventDetailActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name=".loginscreen.LoginScreenActivity"
            android:label="@string/app_name">
        </activity>

    </application>

</manifest>
